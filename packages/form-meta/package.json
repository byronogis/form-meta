{
  "name": "form-meta",
  "type": "module",
  "version": "2.0.2",
  "description": "Type-safe form field meta resolver with flatten/purge for nested schemas",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/form-meta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/form-meta.git"
  },
  "bugs": "https://github.com/byronogis/form-meta/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "start": "jiti src/index.ts",
    "automd": "automd README.md",
    "prepublishOnly": "pnpm run build",
    "typecheck": "tsc --noEmit",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@standard-schema/spec": "catalog:deps",
    "es-toolkit": "catalog:deps",
    "type-fest": "catalog:types",
    "zod": "catalog:deps"
  },
  "devDependencies": {
    "typescript": "catalog:deps"
  }
}
